#. type: SH
#: raw/man1/b2sum.1:3
#, no-wrap
msgid "NAME"
msgstr "名称"

#. type: Plain text
#: raw/man1/b2sum.1:5
msgid "b2sum - compute and check BLAKE2 message digest"
msgstr "b2sum - 计算并校验 BLAKE2 信息摘要"

#. type: SH
#: raw/man1/b2sum.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
#: raw/man1/b2sum.1:8
msgid "B<b2sum> [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr "B<b2sum> [I<\\,选项\\/>]... [I<\\,文件列表\\/>]..."

#. type: SH
#: raw/man1/b2sum.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#. type: Plain text
#: raw/man1/b2sum.1:12
msgid "Print or check BLAKE2 (512-bit) checksums."
msgstr "输出或检查 BLAKE2（512位）校验和。"

#. type: Plain text
#: raw/man1/b2sum.1:14
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

#. type: TP
#: raw/man1/b2sum.1:14
#, no-wrap
msgid "B<-b>, B<--binary>"
msgstr ""

#. type: Plain text
#: raw/man1/b2sum.1:17
msgid "read in binary mode"
msgstr ""

#. type: TP
#: raw/man1/b2sum.1:17
#, no-wrap
msgid "B<-c>, B<--check>"
msgstr ""

#. type: Plain text
#: raw/man1/b2sum.1:20
msgid "read BLAKE2 sums from the FILEs and check them"
msgstr ""

#. type: TP
#: raw/man1/b2sum.1:20
#, no-wrap
msgid "B<-l>, B<--length>"
msgstr ""

#. type: Plain text
#: raw/man1/b2sum.1:24
msgid ""
"digest length in bits; must not exceed the maximum for the blake2 algorithm "
"and must be a multiple of 8"
msgstr ""

#. type: TP
#: raw/man1/b2sum.1:24
#, no-wrap
msgid "B<--tag>"
msgstr ""

#. type: Plain text
#: raw/man1/b2sum.1:27
msgid "create a BSD-style checksum"
msgstr ""

#. type: TP
#: raw/man1/b2sum.1:27
#, no-wrap
msgid "B<-t>, B<--text>"
msgstr ""

#. type: Plain text
#: raw/man1/b2sum.1:30
msgid "read in text mode (default)"
msgstr ""

#. type: SS
#: raw/man1/b2sum.1:30
#, no-wrap
msgid "The following five options are useful only when verifying checksums:"
msgstr ""

#. type: TP
#: raw/man1/b2sum.1:31
#, no-wrap
msgid "B<--ignore-missing>"
msgstr ""

#. type: Plain text
#: raw/man1/b2sum.1:34
msgid "don't fail or report status for missing files"
msgstr ""

#. type: TP
#: raw/man1/b2sum.1:34
#, no-wrap
msgid "B<--quiet>"
msgstr "B<--quiet>"

#. type: Plain text
#: raw/man1/b2sum.1:37
msgid "don't print OK for each successfully verified file"
msgstr "不要为每个成功验证的文件显示成功信息"

#. type: TP
#: raw/man1/b2sum.1:37
#, no-wrap
msgid "B<--status>"
msgstr "B<--status>"

#. type: Plain text
#: raw/man1/b2sum.1:40
msgid "don't output anything, status code shows success"
msgstr "不要输出任何信息，由返回值判断成功与否"

#. type: TP
#: raw/man1/b2sum.1:40
#, no-wrap
msgid "B<--strict>"
msgstr "B<--strict>"

#. type: Plain text
#: raw/man1/b2sum.1:43
msgid "exit non-zero for improperly formatted checksum lines"
msgstr ""

#. type: TP
#: raw/man1/b2sum.1:43
#, no-wrap
msgid "B<-w>, B<--warn>"
msgstr "B<-w>, B<--warn>"

#. type: Plain text
#: raw/man1/b2sum.1:46
msgid "warn about improperly formatted checksum lines"
msgstr "为没有正确格式化的校验和文字行给出警告信息"

#. type: TP
#: raw/man1/b2sum.1:46
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: raw/man1/b2sum.1:49
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#. type: TP
#: raw/man1/b2sum.1:49
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: raw/man1/b2sum.1:52
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#. type: Plain text
#: raw/man1/b2sum.1:57
msgid ""
"The sums are computed as described in RFC 7693.  When checking, the input "
"should be a former output of this program.  The default mode is to print a "
"line with checksum, a space, a character indicating input mode ('*' for "
"binary, \\&' ' for text or where binary is insignificant), and name for each "
"FILE."
msgstr ""

