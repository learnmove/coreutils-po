#, no-wrap
msgid "NAME"
msgstr "名称"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 5
msgid "tee - read from standard input and write to standard output and files"
msgstr "tee - 从标准输入读入并写往标准输出和文件"

#
#. type: SH
# File: raw/man1/tee.1, line: 5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"

#. type: Plain text
# File: raw/man1/tee.1, line: 8
msgid "B<tee> [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr "B<tee> [I<\\,选项\\/>]... [I<\\,文件列表\\/>]..."

#
#. type: SH
# File: raw/man1/tee.1, line: 8
#, no-wrap
msgid "DESCRIPTION"
msgstr "描述"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 12
msgid "Copy standard input to each FILE, and also to standard output."
msgstr "把标准输入的数据复制到文件列表中的每一个文件，同时送往标准输出。"

#
#. type: TP
# File: raw/man1/tee.1, line: 12
#, no-wrap
msgid "B<-a>, B<--append>"
msgstr "B<-a>, B<--append>"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 15
msgid "append to the given FILEs, do not overwrite"
msgstr "追加到给出的文件，而不是覆盖"

#
#. type: TP
# File: raw/man1/tee.1, line: 15
#, no-wrap
msgid "B<-i>, B<--ignore-interrupts>"
msgstr "B<-i>, B<--ignore-interrupts>"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 18
msgid "ignore interrupt signals"
msgstr "忽略中断信号"

#
#. type: TP
# File: raw/man1/tee.1, line: 18
#, no-wrap
msgid "B<-p>"
msgstr "B<-p>"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 21
msgid "diagnose errors writing to non pipes"
msgstr ""

#
#. type: TP
# File: raw/man1/tee.1, line: 21
#, no-wrap
msgid "B<--output-error>[=I<\\,MODE\\/>]"
msgstr "B<--output-error>[=I<\\,模式\\/>]"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 24
msgid "set behavior on write error.  See MODE below"
msgstr ""

#
#. type: TP
# File: raw/man1/tee.1, line: 24
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 27
msgid "display this help and exit"
msgstr "显示此帮助信息并退出"

#
#. type: TP
# File: raw/man1/tee.1, line: 27
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 30
msgid "output version information and exit"
msgstr "显示版本信息并退出"

#
#. type: SS
# File: raw/man1/tee.1, line: 30
#, no-wrap
msgid "MODE determines behavior with write errors on the outputs:"
msgstr ""

#
#. type: TP
# File: raw/man1/tee.1, line: 31
#, no-wrap
msgid "\\&'warn'"
msgstr "\\&'warn'"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 34
msgid "diagnose errors writing to any output"
msgstr ""

#
#. type: TP
# File: raw/man1/tee.1, line: 34
#, no-wrap
msgid "\\&'warn-nopipe'"
msgstr "\\&'warn-nopipe'"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 37
msgid "diagnose errors writing to any output not a pipe"
msgstr ""

#
#. type: TP
# File: raw/man1/tee.1, line: 37
#, no-wrap
msgid "\\&'exit'"
msgstr "\\&'exit'"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 40
msgid "exit on error writing to any output"
msgstr ""

#
#. type: TP
# File: raw/man1/tee.1, line: 40
#, no-wrap
msgid "\\&'exit-nopipe'"
msgstr "\\&'exit-nopipe'"

#
#. type: Plain text
# File: raw/man1/tee.1, line: 43
msgid "exit on error writing to any output not a pipe"
msgstr ""

#
#. type: Plain text
# File: raw/man1/tee.1, line: 48
msgid ""
"The default MODE for the B<-p> option is 'warn-nopipe'.  The default "
"operation when B<--output-error> is not specified, is to exit immediately on "
"error writing to a pipe, and diagnose errors writing to non pipe outputs."
msgstr ""

