msgid "like B<-h>, but use powers of 1000 not 1024"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 85
#, no-wrap
msgid "B<-s>, B<--summarize>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 88
msgid "display only a total for each argument"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 88
#, no-wrap
msgid "B<-t>, B<--threshold>=I<\\,SIZE\\/>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 92
msgid ""
"exclude entries smaller than SIZE if positive, or entries greater than SIZE "
"if negative"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 92
#, no-wrap
msgid "B<--time>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 96
