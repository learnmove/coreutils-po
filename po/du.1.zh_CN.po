#
#. type: Plain text
# File: raw/man1/du.1, line: 15
msgid "Summarize disk usage of the set of FILEs, recursively for directories."
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 17
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr "必选参数对长短选项同时适用。"
#
#. type: TP
# File: raw/man1/du.1, line: 17
#, no-wrap
msgid "B<-0>, B<--null>"
msgstr "B<-0>, B<--null>"
#
#. type: Plain text
# File: raw/man1/du.1, line: 20
msgid "end each output line with NUL, not newline"
msgstr "使每行输出以 NUL 字符结束，不添加换行符"
#
#. type: TP
# File: raw/man1/du.1, line: 20
#, no-wrap
msgid "B<-a>, B<--all>"
msgstr "B<-a>, B<--all>"
#
#. type: Plain text
# File: raw/man1/du.1, line: 23
msgid "write counts for all files, not just directories"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 23
#, no-wrap
msgid "B<--apparent-size>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 29
msgid ""
"print apparent sizes, rather than disk usage; although the apparent size is "
"usually smaller, it may be larger due to holes in ('sparse') files, internal "
"fragmentation, indirect blocks, and the like"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 29
#, no-wrap
msgid "B<-B>, B<--block-size>=I<\\,SIZE\\/>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 34
msgid ""
"scale sizes by SIZE before printing them; e.g., \\&'-BM' prints sizes in "
"units of 1,048,576 bytes; see SIZE format below"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 34
#, no-wrap
msgid "B<-b>, B<--bytes>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 37
msgid "equivalent to '--apparent-size B<--block-size>=I<\\,1\\/>'"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 37
#, no-wrap
msgid "B<-c>, B<--total>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 40
msgid "produce a grand total"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 40
#, no-wrap
msgid "B<-D>, B<--dereference-args>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 44
msgid "dereference only symlinks that are listed on the command line"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 44
#, no-wrap
msgid "B<-d>, B<--max-depth>=I<\\,N\\/>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 50
msgid ""
"print the total for a directory (or file, with B<--all>)  only if it is N or "
"fewer levels below the command line argument; B<--max-depth>=I<\\,0\\/> is "
"the same as B<--summarize>"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 50
#, fuzzy, no-wrap
msgid "B<--files0-from>=I<\\,F\\/>"
msgstr "B<--files0-from>=I<F>"
#
#. type: Plain text
# File: raw/man1/du.1, line: 55
msgid ""
"summarize disk usage of the NUL-terminated file names specified in file F; "
"if F is -, then read names from standard input"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 55
#, no-wrap
msgid "B<-H>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 58
msgid "equivalent to B<--dereference-args> (B<-D>)"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 58
#, no-wrap
msgid "B<-h>, B<--human-readable>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 61
msgid "print sizes in human readable format (e.g., 1K 234M 2G)"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 61
#, no-wrap
msgid "B<--inodes>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 64
msgid "list inode usage information instead of block usage"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 64
#, no-wrap
msgid "B<-k>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 67
msgid "like B<--block-size>=I<\\,1K\\/>"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 67
#, no-wrap
msgid "B<-L>, B<--dereference>"
msgstr "B<-L>, B<--dereference>"
#
#. type: Plain text
# File: raw/man1/du.1, line: 70
msgid "dereference all symbolic links"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 70
#, no-wrap
msgid "B<-l>, B<--count-links>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 73
msgid "count sizes many times if hard linked"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 73
#, no-wrap
msgid "B<-m>"
msgstr "B<-m>"
#
#. type: Plain text
# File: raw/man1/du.1, line: 76
msgid "like B<--block-size>=I<\\,1M\\/>"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 76
#, no-wrap
msgid "B<-P>, B<--no-dereference>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 79
msgid "don't follow any symbolic links (this is the default)"
msgstr ""
#
#. type: TP
# File: raw/man1/du.1, line: 79
#, no-wrap
msgid "B<-S>, B<--separate-dirs>"
msgstr ""
#
#. type: Plain text
# File: raw/man1/du.1, line: 82
