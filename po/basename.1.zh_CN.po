#
#. type: Plain text
# File: raw/man1/basename.1, line: 8
#, fuzzy
msgid "B<basename> I<\\,NAME \\/>[I<\\,SUFFIX\\/>]"
msgstr "B<basename> I<名称 >[I<后缀>]"
#
#. type: Plain text
# File: raw/man1/basename.1, line: 11
#, fuzzy
msgid "B<basename> I<\\,OPTION\\/>... I<\\,NAME\\/>..."
msgstr "B<basename> I<选项>... I<名称>..."
#
#. type: SH
