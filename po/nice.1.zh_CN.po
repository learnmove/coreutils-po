#
#. type: Plain text
# File: raw/man1/nice.1, line: 5
msgid "nice - run a program with modified scheduling priority"
msgstr "nice - 改变执行程序的调度优先级"
#
#. type: SH
# File: raw/man1/nice.1, line: 5
#, no-wrap
msgid "SYNOPSIS"
msgstr "概述"
#
#. type: Plain text
# File: raw/man1/nice.1, line: 8
#, fuzzy
msgid "B<nice> [I<\\,OPTION\\/>] [I<\\,COMMAND \\/>[I<\\,ARG\\/>]...]"
msgstr "B<nice> [I<选项>] [I<命令 >[I<参数>]...]"
#
