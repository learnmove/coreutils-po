msgid "NAME"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:5
msgid "fmt - simple optimal text formatter"
msgstr ""

#. type: SH
#: raw/man1/fmt.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:8
msgid "B<fmt> [I<\\,-WIDTH\\/>] [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr ""

#. type: SH
#: raw/man1/fmt.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:13
msgid ""
"Reformat each paragraph in the FILE(s), writing to standard output.  The "
"option B<-WIDTH> is an abbreviated form of B<--width>=I<\\,DIGITS\\/>."
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:15
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:17
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: raw/man1/fmt.1:17
#, no-wrap
msgid "B<-c>, B<--crown-margin>"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:20
msgid "preserve indentation of first two lines"
msgstr ""

#. type: TP
#: raw/man1/fmt.1:20
#, no-wrap
msgid "B<-p>, B<--prefix>=I<\\,STRING\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:24
msgid ""
"reformat only lines beginning with STRING, reattaching the prefix to "
"reformatted lines"
msgstr ""

#. type: TP
#: raw/man1/fmt.1:24
#, no-wrap
msgid "B<-s>, B<--split-only>"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:27
msgid "split long lines, but do not refill"
msgstr ""

#. type: TP
#: raw/man1/fmt.1:27
#, no-wrap
msgid "B<-t>, B<--tagged-paragraph>"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:30
msgid "indentation of first line different from second"
msgstr ""

#. type: TP
#: raw/man1/fmt.1:30
#, no-wrap
msgid "B<-u>, B<--uniform-spacing>"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:33
msgid "one space between words, two after sentences"
msgstr ""

#. type: TP
#: raw/man1/fmt.1:33
#, no-wrap
msgid "B<-w>, B<--width>=I<\\,WIDTH\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:36
msgid "maximum line width (default of 75 columns)"
msgstr ""

#. type: TP
#: raw/man1/fmt.1:36
#, no-wrap
msgid "B<-g>, B<--goal>=I<\\,WIDTH\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:39
msgid "goal width (default of 93% of width)"
msgstr ""

#. type: TP
#: raw/man1/fmt.1:39
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: raw/man1/fmt.1:42
msgid "display this help and exit"
msgstr ""

#. type: TP
#: raw/man1/fmt.1:42
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
