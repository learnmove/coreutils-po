msgstr ""

#. type: Plain text
#: raw/man1/date.1:5
msgid "date - print or set the system date and time"
msgstr ""

#. type: SH
#: raw/man1/date.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:8
msgid "B<date> [I<\\,OPTION\\/>]... [I<\\,+FORMAT\\/>]"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:11
msgid ""
"B<date> [I<\\,-u|--utc|--universal\\/>] [I<\\,MMDDhhmm\\/>[[I<\\,CC\\/>]I<\\,"
"YY\\/>][I<\\,.ss\\/>]]"
msgstr ""

#. type: SH
#: raw/man1/date.1:11
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:15
msgid "Display the current time in the given FORMAT, or set the system date."
msgstr ""

#. type: Plain text
#: raw/man1/date.1:17
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: raw/man1/date.1:17
#, no-wrap
msgid "B<-d>, B<--date>=I<\\,STRING\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:20
msgid "display time described by STRING, not 'now'"
msgstr ""

#. type: TP
#: raw/man1/date.1:20
#, no-wrap
msgid "B<--debug>"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:24
msgid "annotate the parsed date, and warn about questionable usage to stderr"
msgstr ""

#. type: TP
#: raw/man1/date.1:24
#, no-wrap
msgid "B<-f>, B<--file>=I<\\,DATEFILE\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:27
msgid "like B<--date>; once for each line of DATEFILE"
msgstr ""

#. type: TP
#: raw/man1/date.1:27
#, no-wrap
msgid "B<-I[FMT]>, B<--iso-8601>[=I<\\,FMT\\/>]"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:34
msgid ""
"output date/time in ISO 8601 format.  FMT='date' for date only (the "
"default), \\&'hours', 'minutes', 'seconds', or 'ns' for date and time to the "
"indicated precision.  Example: 2006-08-14T02:34:56-06:00"
msgstr ""

#. type: TP
#: raw/man1/date.1:34
#, no-wrap
msgid "B<-R>, B<--rfc-2822>"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:38
msgid ""
"output date and time in RFC 2822 format.  Example: Mon, 14 Aug 2006 02:34:56 "
"B<-0600>"
msgstr ""

#. type: TP
#: raw/man1/date.1:38
#, no-wrap
msgid "B<--rfc-3339>=I<\\,FMT\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:44
msgid ""
"output date/time in RFC 3339 format.  FMT='date', 'seconds', or 'ns' for "
"date and time to the indicated precision.  Example: 2006-08-14 02:34:56-06:00"
msgstr ""

#. type: TP
#: raw/man1/date.1:44
#, no-wrap
msgid "B<-r>, B<--reference>=I<\\,FILE\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:47
msgid "display the last modification time of FILE"
msgstr ""

#. type: TP
#: raw/man1/date.1:47
#, no-wrap
msgid "B<-s>, B<--set>=I<\\,STRING\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:50
msgid "set time described by STRING"
msgstr ""

#. type: TP
#: raw/man1/date.1:50
#, no-wrap
msgid "B<-u>, B<--utc>, B<--universal>"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:53
msgid "print or set Coordinated Universal Time (UTC)"
msgstr ""

#. type: TP
#: raw/man1/date.1:53
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:56
msgid "display this help and exit"
msgstr ""

#. type: TP
#: raw/man1/date.1:56
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:59
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:61
msgid "FORMAT controls the output.  Interpreted sequences are:"
msgstr ""

#. type: TP
#: raw/man1/date.1:61
#, no-wrap
msgid "%%"
msgstr ""

#. type: Plain text
#: raw/man1/date.1:64
msgid "a literal %"
msgstr ""

#. type: TP
