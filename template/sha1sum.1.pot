msgid "sha1sum - compute and check SHA1 message digest"
msgstr ""

#. type: SH
#: raw/man1/sha1sum.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:8
msgid "B<sha1sum> [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr ""

#. type: SH
#: raw/man1/sha1sum.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:12
msgid "Print or check SHA1 (160-bit) checksums."
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:14
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:14
#, no-wrap
msgid "B<-b>, B<--binary>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:17
msgid "read in binary mode"
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:17
#, no-wrap
msgid "B<-c>, B<--check>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:20
msgid "read SHA1 sums from the FILEs and check them"
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:20
#, no-wrap
msgid "B<--tag>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:23
msgid "create a BSD-style checksum"
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:23
#, no-wrap
msgid "B<-t>, B<--text>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:26
msgid "read in text mode (default)"
msgstr ""

#. type: SS
#: raw/man1/sha1sum.1:26
#, no-wrap
msgid "The following five options are useful only when verifying checksums:"
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:27
#, no-wrap
msgid "B<--ignore-missing>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:30
msgid "don't fail or report status for missing files"
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:30
#, no-wrap
msgid "B<--quiet>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:33
msgid "don't print OK for each successfully verified file"
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:33
#, no-wrap
msgid "B<--status>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:36
msgid "don't output anything, status code shows success"
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:36
#, no-wrap
msgid "B<--strict>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:39
msgid "exit non-zero for improperly formatted checksum lines"
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:39
#, no-wrap
msgid "B<-w>, B<--warn>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:42
msgid "warn about improperly formatted checksum lines"
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:42
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:45
msgid "display this help and exit"
msgstr ""

#. type: TP
#: raw/man1/sha1sum.1:45
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:48
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: raw/man1/sha1sum.1:53
msgid ""
"The sums are computed as described in FIPS-180-1.  When checking, the input "
"should be a former output of this program.  The default mode is to print a "
"line with checksum, a space, a character indicating input mode ('*' for "
"binary, \\&' ' for text or where binary is insignificant), and name for each "
"FILE."
msgstr ""

