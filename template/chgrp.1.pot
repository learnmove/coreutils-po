#. type: SH
#: raw/man1/chgrp.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:5
msgid "chgrp - change group ownership"
msgstr ""

#. type: SH
#: raw/man1/chgrp.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:8
msgid "B<chgrp> [I<\\,OPTION\\/>]... I<\\,GROUP FILE\\/>..."
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:11
msgid "B<chgrp> [I<\\,OPTION\\/>]... I<\\,--reference=RFILE FILE\\/>..."
msgstr ""

#. type: SH
#: raw/man1/chgrp.1:11
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:16
msgid ""
"Change the group of each FILE to GROUP.  With B<--reference>, change the "
"group of each FILE to that of RFILE."
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:16
#, no-wrap
msgid "B<-c>, B<--changes>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:19
msgid "like verbose but report only when a change is made"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:19
#, no-wrap
msgid "B<-f>, B<--silent>, B<--quiet>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:22
msgid "suppress most error messages"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:22
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:25
msgid "output a diagnostic for every file processed"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:25
#, no-wrap
msgid "B<--dereference>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:29
msgid ""
"affect the referent of each symbolic link (this is the default), rather than "
"the symbolic link itself"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:29
#, no-wrap
msgid "B<-h>, B<--no-dereference>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:34
msgid ""
"affect symbolic links instead of any referenced file (useful only on systems "
"that can change the ownership of a symlink)"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:34
#, no-wrap
msgid "B<--no-preserve-root>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:37
msgid "do not treat '/' specially (the default)"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:37
#, no-wrap
msgid "B<--preserve-root>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:40
msgid "fail to operate recursively on '/'"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:40
#, no-wrap
msgid "B<--reference>=I<\\,RFILE\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:44
msgid "use RFILE's group rather than specifying a GROUP value"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:44
#, no-wrap
msgid "B<-R>, B<--recursive>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:47
msgid "operate on files and directories recursively"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:51
msgid ""
"The following options modify how a hierarchy is traversed when the B<-R> "
"option is also specified.  If more than one is specified, only the final one "
"takes effect."
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:51
#, no-wrap
msgid "B<-H>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:55
msgid ""
"if a command line argument is a symbolic link to a directory, traverse it"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:55
#, no-wrap
msgid "B<-L>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:59
msgid "traverse every symbolic link to a directory encountered"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:59
#, no-wrap
msgid "B<-P>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:62
msgid "do not traverse any symbolic links (default)"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:62
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:65
msgid "display this help and exit"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:65
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:68
msgid "output version information and exit"
msgstr ""

#. type: SH
#: raw/man1/chgrp.1:68
#, no-wrap
msgid "EXAMPLES"
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:69
#, no-wrap
msgid "chgrp staff /u"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:72
msgid "Change the group of /u to \"staff\"."
msgstr ""

#. type: TP
#: raw/man1/chgrp.1:72
#, no-wrap
msgid "chgrp -hR staff /u"
msgstr ""

#. type: Plain text
#: raw/man1/chgrp.1:75
msgid "Change the group of /u and subfiles to \"staff\"."
msgstr ""

