msgstr ""

#. type: SH
#: raw/man1/split.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:5
msgid "split - split a file into pieces"
msgstr ""

#. type: SH
#: raw/man1/split.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:8
msgid "B<split> [I<\\,OPTION\\/>]... [I<\\,FILE \\/>[I<\\,PREFIX\\/>]]"
msgstr ""

#. type: SH
#: raw/man1/split.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:13
msgid ""
"Output pieces of FILE to PREFIXaa, PREFIXab, ...; default size is 1000 "
"lines, and default PREFIX is 'x'."
msgstr ""

#. type: Plain text
#: raw/man1/split.1:15
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

#. type: Plain text
#: raw/man1/split.1:17
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#. type: TP
#: raw/man1/split.1:17
#, no-wrap
msgid "B<-a>, B<--suffix-length>=I<\\,N\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:20
msgid "generate suffixes of length N (default 2)"
msgstr ""

#. type: TP
#: raw/man1/split.1:20
#, no-wrap
msgid "B<--additional-suffix>=I<\\,SUFFIX\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:23
msgid "append an additional SUFFIX to file names"
msgstr ""

#. type: TP
#: raw/man1/split.1:23
#, no-wrap
msgid "B<-b>, B<--bytes>=I<\\,SIZE\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:26
msgid "put SIZE bytes per output file"
msgstr ""

#. type: TP
#: raw/man1/split.1:26
#, no-wrap
msgid "B<-C>, B<--line-bytes>=I<\\,SIZE\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:29
msgid "put at most SIZE bytes of records per output file"
msgstr ""

#. type: TP
#: raw/man1/split.1:29
#, no-wrap
msgid "B<-d>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:32
msgid "use numeric suffixes starting at 0, not alphabetic"
msgstr ""

#. type: TP
#: raw/man1/split.1:32
#, no-wrap
msgid "B<--numeric-suffixes>[=I<\\,FROM\\/>]"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:35
msgid "same as B<-d>, but allow setting the start value"
msgstr ""

#. type: TP
#: raw/man1/split.1:35
#, no-wrap
msgid "B<-e>, B<--elide-empty-files>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:38
msgid "do not generate empty output files with '-n'"
msgstr ""

#. type: TP
#: raw/man1/split.1:38
#, no-wrap
msgid "B<--filter>=I<\\,COMMAND\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:41
msgid "write to shell COMMAND; file name is $FILE"
msgstr ""

#. type: TP
#: raw/man1/split.1:41
#, no-wrap
msgid "B<-l>, B<--lines>=I<\\,NUMBER\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:44
msgid "put NUMBER lines/records per output file"
msgstr ""

#. type: TP
#: raw/man1/split.1:44
#, no-wrap
msgid "B<-n>, B<--number>=I<\\,CHUNKS\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:47
msgid "generate CHUNKS output files; see explanation below"
msgstr ""

#. type: TP
#: raw/man1/split.1:47
#, no-wrap
msgid "B<-t>, B<--separator>=I<\\,SEP\\/>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:51
msgid ""
"use SEP instead of newline as the record separator; \\&'\\e0' (zero) "
"specifies the NUL character"
msgstr ""

#. type: TP
#: raw/man1/split.1:51
#, no-wrap
msgid "B<-u>, B<--unbuffered>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:54
msgid "immediately copy input to output with '-n r/...'"
msgstr ""

#. type: TP
#: raw/man1/split.1:54
#, no-wrap
msgid "B<--verbose>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:58
msgid "print a diagnostic just before each output file is opened"
msgstr ""

#. type: TP
#: raw/man1/split.1:58
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:61
msgid "display this help and exit"
msgstr ""

#. type: TP
#: raw/man1/split.1:61
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:64
msgid "output version information and exit"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:67
msgid ""
"The SIZE argument is an integer and optional unit (example: 10K is "
"10*1024).  Units are K,M,G,T,P,E,Z,Y (powers of 1024) or KB,MB,... (powers "
"of 1000)."
msgstr ""

#. type: SS
#: raw/man1/split.1:67
#, no-wrap
msgid "CHUNKS may be:"
msgstr ""

#. type: TP
#: raw/man1/split.1:68
#, no-wrap
msgid "N"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:71
msgid "split into N files based on size of input"
msgstr ""

#. type: TP
#: raw/man1/split.1:71
#, no-wrap
msgid "K/N"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:74
msgid "output Kth of N to stdout"
msgstr ""

#. type: TP
#: raw/man1/split.1:74
#, no-wrap
msgid "l/N"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:77
msgid "split into N files without splitting lines/records"
msgstr ""

#. type: TP
#: raw/man1/split.1:77
#, no-wrap
msgid "l/K/N"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:80
msgid "output Kth of N to stdout without splitting lines/records"
msgstr ""

#. type: TP
#: raw/man1/split.1:80
#, no-wrap
msgid "r/N"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:83
msgid "like 'l' but use round robin distribution"
msgstr ""

#. type: TP
#: raw/man1/split.1:83
#, no-wrap
msgid "r/K/N"
msgstr ""

#. type: Plain text
#: raw/man1/split.1:86
msgid "likewise but only output Kth of N to stdout"
msgstr ""

