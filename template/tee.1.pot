#. type: SH
#: raw/man1/tee.1:3
#, no-wrap
msgid "NAME"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:5
msgid "tee - read from standard input and write to standard output and files"
msgstr ""

#. type: SH
#: raw/man1/tee.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:8
msgid "B<tee> [I<\\,OPTION\\/>]... [I<\\,FILE\\/>]..."
msgstr ""

#. type: SH
#: raw/man1/tee.1:8
#, no-wrap
msgid "DESCRIPTION"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:12
msgid "Copy standard input to each FILE, and also to standard output."
msgstr ""

#. type: TP
#: raw/man1/tee.1:12
#, no-wrap
msgid "B<-a>, B<--append>"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:15
msgid "append to the given FILEs, do not overwrite"
msgstr ""

#. type: TP
#: raw/man1/tee.1:15
#, no-wrap
msgid "B<-i>, B<--ignore-interrupts>"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:18
msgid "ignore interrupt signals"
msgstr ""

#. type: TP
#: raw/man1/tee.1:18
#, no-wrap
msgid "B<-p>"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:21
msgid "diagnose errors writing to non pipes"
msgstr ""

#. type: TP
#: raw/man1/tee.1:21
#, no-wrap
msgid "B<--output-error>[=I<\\,MODE\\/>]"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:24
msgid "set behavior on write error.  See MODE below"
msgstr ""

#. type: TP
#: raw/man1/tee.1:24
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:27
msgid "display this help and exit"
msgstr ""

#. type: TP
#: raw/man1/tee.1:27
#, no-wrap
msgid "B<--version>"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:30
msgid "output version information and exit"
msgstr ""

#. type: SS
#: raw/man1/tee.1:30
#, no-wrap
msgid "MODE determines behavior with write errors on the outputs:"
msgstr ""

#. type: TP
#: raw/man1/tee.1:31
#, no-wrap
msgid "\\&'warn'"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:34
msgid "diagnose errors writing to any output"
msgstr ""

#. type: TP
#: raw/man1/tee.1:34
#, no-wrap
msgid "\\&'warn-nopipe'"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:37
msgid "diagnose errors writing to any output not a pipe"
msgstr ""

#. type: TP
#: raw/man1/tee.1:37
#, no-wrap
msgid "\\&'exit'"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:40
msgid "exit on error writing to any output"
msgstr ""

#. type: TP
#: raw/man1/tee.1:40
#, no-wrap
msgid "\\&'exit-nopipe'"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:43
msgid "exit on error writing to any output not a pipe"
msgstr ""

#. type: Plain text
#: raw/man1/tee.1:48
msgid ""
"The default MODE for the B<-p> option is 'warn-nopipe'.  The default "
"operation when B<--output-error> is not specified, is to exit immediately on "
"error writing to a pipe, and diagnose errors writing to non pipe outputs."
msgstr ""

