msgstr ""

#. type: Plain text
#: raw/man1/nohup.1:15
msgid "Run COMMAND, ignoring hangup signals."
msgstr ""

#. type: TP
#: raw/man1/nohup.1:15
#, no-wrap
msgid "B<--help>"
msgstr ""

#. type: Plain text
#: raw/man1/nohup.1:18
msgid "display this help and exit"
msgstr ""

