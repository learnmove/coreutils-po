#: raw/man1/nohup.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr ""

#. type: Plain text
#: raw/man1/nohup.1:8
msgid "B<nohup> I<\\,COMMAND \\/>[I<\\,ARG\\/>]..."
msgstr ""

#. type: Plain text
#: raw/man1/nohup.1:11
